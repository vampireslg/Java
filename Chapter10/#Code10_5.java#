import java.io.*;
public class Code10_5{
    public static void main(String[] args){
	int b;
        byte buffer[]=new byte[39];
	try{System.out.println("Pls input a line of words ,it will be stored in writeout.txt");
	    b=System.in.read(buffer);
	    FileOutputStream writefile=new FileOutput Stream("writeout.txt");
	    writefile.write(buffer,0,b);
	}
	catch(IOException e){
	    System.out.println("Error : " + e);
	}
    }
}
	