package smartalgorithm;
import java.util.*;

public class HashSort {
	int nodeNum; 
	void filterAndPrint(HashMap hm){
		
	}
	void initHashMap(int[][] initArr){
		//locMap Key : location ; Value : node's value .
		LinkedList<Integer> criticalL =  CriticalPath.searchCriticalPath(initArr);
		int csize = criticalL.size();
		HashMap<Integer, Integer> locMap = new HashMap<>();
		int con = 0 ;
		for (int item : criticalL){
			locMap.put(con ++ , item);
		}
		
		
		
	}

}
