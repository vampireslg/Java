class Fatherclass{
	float methodf(float x, float y){
		return x*y;
	}
}
class Childclass extends Fatherclass{
	float methodf(float x, int y){
		return x;
	}
}
public class Mark {
	public static void main(String args[]){
		Childclass n=new Childclass();
		float c=n.methodf(4, 6);
		System.out.println(c);
	}

}
