import java.util.*;
public class Work5 {
	public static void concatString(String str1, String str2) {
		String str = str1.concat(str2);
		System.out.println(str);
	}

	public static void main(String[] args) {
		String s1 = "Hello Sister ";
		String s2 = "\nHello again Sister .", s3;
		concatString(s1, s2);
		int a[]={23,34,24,284,-23,29};
		double b[]={12.23, 3.23, 3.45,2.34,23.41,39.234,239.44};
		Arrays.sort(a);Arrays.sort(b,1,4);
		for (int i=0;i<=5;i++)
			System.out.println(a[i]+",");
		for (int i=0;i<b.length;i++)
			System.out.println(b[i]+",");
		
		char a1[]={'a','b','c','d','e'},b1[]={'1','2','3','4','5'};
		System.arraycopy(a1, 2, b1, 1, a1.length-1);
		System.out.println(new String(a1));
		System.out.println(new String(b1));
		
		
		
		
		
	}
}
