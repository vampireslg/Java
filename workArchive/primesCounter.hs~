--use Mod method to computer the number of primes 
--in some range

primesNum :: [Integer] -> Integer
primesNum [] = toInteger 0
primesNum (x,xs)	 | isPrime x = toInteger 1 +(primesNum xs)
	  		 | otherwise = primesNum xs

isPrime :: Integer -> Bool
isPrime num	   | num == 1 = False
		   | num == 2|| num == 3 = True
		   | div' num [2..sqrtnum] == 0 = False
		     where sqrtnum = toInteger (sqrt(num))
		     	   div' x (y:ys)       | ys = [] = div x y
			   	  	       | div x y == 0 = 0 
			   	  	       | otherwise = div' x ys
		   | otherwise = True