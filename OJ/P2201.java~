import java.io.*;
import java.util.*;

public class P2201{
    public static void main(String[] args){
	Scanner lines = new Scanner(System.in);
	int nu = lines.nextInt();

	while(nu > 0){
	    Scanner in = new Scanner(System.in);
	    int lnu = in.nextInt();
	    String co = in.next();
	    int rnu = in.nextInt();
	    if (lnu < rnu )
		System.out.println("NO BRAINS");
	    else
		System.out.println("MMM BRAINS");
	    
	    nu--;
	}
    }
}
